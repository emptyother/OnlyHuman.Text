{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"command": "dotnet build",
			"type": "shell",
			"group": "build",
			"presentation": {
				"reveal": "silent"
			},
			"problemMatcher": "$msCompile"
		},
		{
			"type": "script",
			"label": "Run Test",
			"scriptType": "powershell",
			"cmdLine": "powershell .\\test.ps1 --NoProfile --NoLogo",
			"path": "/",
			"fileName": "test.ps1",
			"scriptFile": true,
			"requiresArgs": false,
			"uri": {
				"$mid": 1,
				"fsPath": ".\\Tests\\test.ps1",
				"_sep": 1,
				"path": "./Tests/test.ps1",
				"scheme": "file"
			},
			"problemMatcher": []
		}
	]
}